{
    "steps": [
        {
            "step_number": 1,
            "description": "Read the content of the file at F:/numbers.txt",
            "agent_type": "CRUDAgent",
            "execution_mode": "static",
            "action_type": {
                "agent": "CRUDAgent",
                "tool": "read_file"
            },
            "params": {
                "filename": "F:/numbers.txt"
            },
            "conditions": [],
            "retry": 0,
            "store_result_as": "file_content",
            "next_step_on_success": 2,
            "next_step_on_failure": null
        },
        {
            "step_number": 2,
            "description": "Extract the first number from the file content",
            "agent_type": "CRUDAgent",
            "execution_mode": "dynamic",
            "action_type": null,
            "params": {
                "filename": "<file_content>.content"
            },
            "conditions": [],
            "retry": 0,
            "store_result_as": "first_number",
            "next_step_on_success": 3,
            "next_step_on_failure": null
        },
        {
            "step_number": 3,
            "description": "Calculate the square of the extracted number",
            "agent_type": "SimpleMathAgent",
            "execution_mode": "static",
            "action_type": {
                "agent": "SimpleMathAgent",
                "tool": "square"
            },
            "params": {
                "n": "<first_number>.output.result"
            },
            "conditions": [],
            "retry": 0,
            "store_result_as": "square_result",
            "next_step_on_success": 4,
            "next_step_on_failure": null
        },
        {
            "step_number": 4,
            "description": "Create F:/result.txt and write the square result into it",
            "agent_type": "CRUDAgent",
            "execution_mode": "static",
            "action_type": {
                "agent": "CRUDAgent",
                "tool": "create_file"
            },
            "params": {
                "filename": "F:/result.txt",
                "content": "<square_result>.output.result",
                "type_file": ".txt"
            },
            "conditions": [],
            "retry": 0,
            "store_result_as": "final_result",
            "next_step_on_success": null,
            "next_step_on_failure": null
        }
    ]
